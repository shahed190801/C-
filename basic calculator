#include <iostream>
using namespace std;
int main()
{
 int a,b,sum,sub,mul,div,rem;
 
	cout<< "a=";
  	 cin>> a;
 	cout<< "b=";
   	 cin>> b;
   	 
 	sum= a+b;
 	sub= a-b;
 	div= a/b;
 	rem= a%b;
 	mul= a*b;
 	
 cout<< "sum:"<< sum << endl;
  cout<< "sub:"<< sub << endl;
   cout<< "div:"<< div <<" rem:" <<rem<< endl;
    cout<< "mul:"<< mul << endl;
 
return 0;
}
